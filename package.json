{
  "name": "dock108-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=20.11.1"
  },
  "scripts": {
    "lint": "eslint \"apps/**/*.{js,jsx,ts,tsx}\" \"packages/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "test": "pnpm -r --if-present test",
    "dev": "pnpm -r --parallel dev --filter ./apps/*"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
